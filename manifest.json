{
  "name": "telegram-ultra",
  "author": "@KingSurf7",
  "version": "Beta-1.0",
  "scripts": [{
    "event": "onResolve",
    "match": {
      "urls": [
        "*://t.me/*",
        "*://telegram.org/*",
        "*://cdn*.telegram.org/*"
      ]
    },
    "entry": "index.js"
  }],
  "settings": [{
    "auto_update": true
  }] 
}
