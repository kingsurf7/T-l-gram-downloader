{
  "name": "telegram-ultra",
  "title": "Telegram Downloader",
  "description": "Download files from Telegram links",
  "author": "@KingSurf7",
  "version": "1.0.0",
  "scripts": [
    {
      "event": "onResolve",
      "match": {
        "urls": [
          "*://t.me/*",
          "*://telegram.org/*",
          "*://cdn*.telegram-cdn.org/*"
        ]
      },
      "entry": "index.js"
    }
  ],
  "settings": [{
    "auto_update": true
  }],
  
}
