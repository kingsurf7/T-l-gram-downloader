{
  "name": "telegram-ultra",
  "title": "Telegram Downloader",
  "description": "Download Telegram files by pasting t.me links.",
  "author": "@KingSurf7",
  "version": "1.0.0",
  "script": {
    "event": "onResolve",
    "match": [
      {
        "urls": ["*://t.me/*"]
      }
    ],
    "entry": "index.js"
  },
  "settings":[
    {
      "name": "auto_update",
      "title": "Auto Update",
      "description": "Enable automatic updates",
      "type": "boolean",
      "value": true
    }
   ] 
}
